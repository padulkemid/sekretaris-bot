{
  "name": "sekretaris",
  "version": "1.0.0",
  "description": "Sekretaris Discord Bot",
  "author": "Fadhil Muhammad <gue@padulkem.id>",
  "license": "MIT",
  "engines": {
    "node": ">=19.4.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "test": "jest",
    "start": "yarn run build && node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --quiet --fix",
    "test:cov": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false --silent --coverage --noStackTrace --maxWorkers=50%"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test:cov"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "rome format --write",
      "git add"
    ]
  },
  "dependencies": {
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "fastify": "^4.11.0",
    "joi": "^17.7.0",
    "lodash": "^4.17.21",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.26",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-jasmine2": "^29.3.1",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.1.0",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.3",
    "rome": "^11.0.0",
    "sucrase": "^3.29.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
